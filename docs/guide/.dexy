{
    "$reset" : {},
    "@list-files.sh|jinja|sh" : { "propagate" : true, "contents" : "cd ../{{ s.canonical_dir() }}; tree -a -i -f -n -I config.dexy --noreport" },
    "index.html|hd|ft|jinja" : {
        "allinputs" : true,
        "priority" : 10,
        "header" : "_header.html",
        "footer" : "_footer.html"
    },
    "index.md|jinja|markdown|hd|ft|jinja" : {
        "allinputs" : true,
        "priority" : 10,
        "header" : "_header.html",
        "footer" : "_footer.html",
        "inputs" : ["dexy-source.py|fn|py"]
    },
    "*.html|pyg|-" : {
        "postfix" : "-src.html",
        "final" : true,
        "pygments" : { "noclasses" : true }
    },
    "*.md|pyg|-" : {
        "postfix" : "-src.html",
        "final" : true,
        "pygments" : { "noclasses" : true }
    },
    "config.dexy|ppjson|pyg" : {}
}
